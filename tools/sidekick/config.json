{
    "project":"Demo Edge Delivery Services",
    "plugins": [
    {
        "id": "asset-library",
        "title": "My Assets",
        "environments": [
        "edit"
        ],
        "url": "https://experience.adobe.com/solutions/CQ-assets-selectors/static-assets/resources/franklin/asset-selector.html?rail=false",
        "isPalette": true,
        "includePaths": [ "**.docx**" ],
        "passConfig": true,
        "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:400px; height: calc(100vh - 60px)",
        "copyMode": [{
            "mimeType": "image/*",
            "value": "reference"
        }]	
    },
    {
        "id": "contactus",
        "title": "Contact US",
        "environments": [ "edit","preview"],
        "url": "/tools/contactus/contactus.html",
        "passConfig": true,
        "passReferrer":true
    },
    {
        "id": "generate-variations",
        "title": "Generate Variations",
        "url": "https://experience.adobe.com/aem/generate-variations",
        "passConfig": true,
        "environments": ["preview","live", "edit"],
        "includePaths": ["**.docx**"]
      }
]
}